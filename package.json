{
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "cypress": "^13.9.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-real-events": "^1.12.0",
    "cypress-wait-until": "^3.0.1",
    "fs-extra": "^11.2.0"
  },
  "scripts": {
    "json:report": "npx mochawesome-merge 'cypress/results/.jsons/*.json'  > mochawesome.json",
    "overAllReport": "npx marge ./mochawesome.json --reportDir cypress/results ",
    "cypress:spec": "npx cypress run --spec cypress/e2e/productSelection.spec.cy.js",
    "cypress:result": "npm run json:report && npm run overAllReport",
    "cypress:run": "npm run cypress:spec && npm run cypress:result"
  }
}
